<iis:Kit xmlns:iis="http://www.xsede.org/iis">
  <Name>science-gateway</Name>
  <Version>5.0.1</Version>
  <Release>r1</Release>
  <Fixed>true</Fixed>
  <Service>
    <Name>ws-gram</Name>
    <Required>true</Required>
    <Variable Name="scheduler">Fork|Condor|LoadLeveler|LSF|PBS|SGE</Variable>
  </Service>
  <Service>
    <Name>prews-gram</Name>
    <Required>true</Required>
    <Variable Name="scheduler">fork|condor|loadleveler|lsf|pbs|sge</Variable>
  </Service>
  <Service>
    <Name>gram5</Name>
    <Required>false</Required>
    <Variable Name="scheduler">fork|condor|loadleveler|lsf|pbs|sge</Variable>
  </Service>
  <Service>
    <Name>ws-delegation</Name>
    <Required>true</Required>
  </Service>
  <Service>
    <Name>ws-ogsa-dai</Name>
    <Required>true</Required>
  </Service>
  <Service>
    <Name>globus-mds</Name>
    <Required>true</Required>
  </Service>
  <Service>
    <Name>globus-mds-secure</Name>
    <Required>true</Required>
  </Service>
  <Software>
    <Name>gridshib-gt</Name>
    <Required>true</Required>
  </Software>
  <Software>
    <Name>commsh</Name>
    <Required>false</Required>
  </Software>
</iis:Kit>
