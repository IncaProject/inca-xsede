<iis:Kit xmlns:iis="http://www.xsede.org/iis">
  <Name>science-gateway</Name>
  <Version>5.0.2</Version>
  <Release>r1</Release>
  <Fixed>true</Fixed>
  <Service>
    <Name>prews-gram</Name>
    <Required>true</Required>
    <Variable Name="scheduler">condor|loadleveler|lsf|pbs|sge|fork</Variable>
    <Group Name="scheduler-batch">condor|loadleveler|lsf|pbs|sge</Group>
  </Service>
  <Service>
    <Name>gram5</Name>
    <Required>false</Required>
    <Variable Name="scheduler">condor|loadleveler|lsf|pbs|sge|fork|slurm</Variable>
    <Group Name="scheduler-batch">condor|loadleveler|lsf|pbs|sge|slurm</Group>
  </Service>
  <Service>
    <Name>ws-delegation</Name>
    <Required>true</Required>
  </Service>
  <Service>
    <Name>ogsa-dai/TeraGridResource</Name>
    <Required>false</Required>
  </Service>
  <Software>
    <Name>commsh</Name>
    <Required>false</Required>
  </Software>
</iis:Kit>
