<iis:Kit xmlns:iis="http://www.xsede.org/iis">
  <Name>remote-compute</Name>
  <Version>4.0.2</Version>
  <Release>r1</Release>
  <Fixed>true</Fixed>
  <Service>
    <Name>globus-mds</Name>
    <Required>true</Required>
  </Service>
  <Service>
    <Name>globus-mds-secure</Name>
    <Required>true</Required>
  </Service>
  <Service>
    <Name>ws-delegation</Name>
    <Required>true</Required>
  </Service>
  <Service>
    <Name>ws-ogsa-dai</Name>
    <Required>true</Required>
  </Service>
  <Service>
    <Name>ws-gram</Name>
    <Required>false</Required>
    <Variable Name="scheduler">Fork|Condor|LoadLeveler|LSF|PBS|SGE</Variable>
  </Service>
  <Service>
    <Name>prews-gram</Name>
    <Required>false</Required>
    <Variable Name="scheduler">Fork|Condor|LoadLeveler|LSF|PBS|SGE</Variable>
  </Service>
</iis:Kit>
