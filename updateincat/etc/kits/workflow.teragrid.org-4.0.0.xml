    <HandleKey>condor-g-[\d\.]+</HandleKey>
  </Software>
  <Software>
    <Name>mycluster</Name>
    <Version>[\d\.\-]+</Version>
    <Required>true</Required>
    <HandleKey>gridshell-[\d\.\-]+</HandleKey>
  </Software>
</iis:Kit>
