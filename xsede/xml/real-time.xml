<stack>
    <id>Real-Time Monitoring Testbed</id>
    <category>
        <id>GRAM</id>
        <package>
            <id>wsgram</id>
            <tests>
                <unitalias>wsgram-external-ping</unitalias>
                <unitalias>wsgram-external-telnet</unitalias>
                <unitalias>wsgram-netstat</unitalias>
                <unitalias>wsgram-wsrf-get-property</unitalias>
                <unitalias>wsgram-fork</unitalias>
                <unitalias>wsgram-batch-submit</unitalias>
                <unitalias>wsgram-batch</unitalias>
            </tests>
        </package>
    </category>
    <category>
        <id>DATA TRANSFER</id>
        <package>
            <id>rft</id>
            <tests>
                <unitalias>rft-version-tyr12.cs.indiana.edu:8443</unitalias>
            </tests>
        </package>
        <package>
            <id>gridftp</id>
            <tests>
                <unitalias>gridftp-external-ping</unitalias>
                <unitalias>gridftp-external-telnet</unitalias>
                <unitalias>gridftp-auth</unitalias>
                <unitalias>gridftp-transfer-/cfs/scratch/users</unitalias>
                <unitalias>gridftp-transfer-/gpfs_scratch1</unitalias>
                <unitalias>gridftp-transfer-/N/dc/scratch</unitalias>
                <unitalias>gridftp-to-gridftp-hg.ncsa.teragrid.org:2812/gpfs_scratch1</unitalias>
                <unitalias>gridftp-to-gridftp.bigred.iu.teragrid.org:2812/N/dc/scratch</unitalias>
                <unitalias>java-cog-bigred-mercury</unitalias>
                <unitalias>java-cog-mercury-bigred</unitalias>
            </tests>
        </package>
    </category>
    <endpoint>
      <nickname>indiana-bigred</nickname>
      <regex>(gridftp.bigred.iu.teragrid.org|gridftp.bigred.iu.teragrid.org 2812|g1.bigred.teragrid.iu.edu|g1.bigred.teragrid.iu.edu 8445)</regex>
    </endpoint>
    <endpoint>
      <nickname>ncsa-grid-abe</nickname>
      <regex>(gridftp-abe.ncsa.teragrid.org|gridftp-abe.ncsa.teragrid.org 2812|grid-abe.ncsa.teragrid.org|grid-abe.ncsa.teragrid.org 8443)</regex>
    </endpoint>
    <endpoint>
      <nickname>ncsa-grid-hg</nickname>
      <regex>(gridftp-hg.ncsa.teragrid.org|gridftp-hg.ncsa.teragrid.org 2812|grid-hg.ncsa.teragrid.org|grid-hg.ncsa.teragrid.org 8444)</regex>
    </endpoint>
    <endpoint>
      <nickname>sdsc-ia64</nickname>
      <regex>(tg-gridftp.sdsc.teragrid.org|tg-gridftp.sdsc.teragrid.org 2811|tg-login1.sdsc.teragrid.org|tg-login1.sdsc.teragrid.org 2119|tg-login1.sdsc.teragrid.org 8443)</regex>
    </endpoint>
</stack>
