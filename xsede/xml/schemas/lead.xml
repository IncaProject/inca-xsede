<ld:leadDefinition xmlns:ld="http://mds.teragrid.org/2007/02/lead">
  <developerName>Suresh Marru</developerName>
  <developerEmail>smarru@cs.indiana.edu</developerEmail>
  <services>
    <service>
      <kitName>remote-compute.teragrid.org</kitName>
      <kitVersion>4.0.2</kitVersion>
      <resource>grid-abe.ncsa.teragrid.org</resource>
      <job>
        <endpoint>https://grid-abe.ncsa.teragrid.org:8443/wsrf/services/ManagedJobFactoryService</endpoint>
        <numCPUs>2</numCPUs>
        <workingDir>/cfs/scratch/</workingDir>
      </job>
    </service>
    <service>
      <kitName>data-movement.teragrid.org</kitName>
      <kitVersion>4.1.1</kitVersion>
      <resource>grid-abe.ncsa.teragrid.org</resource>
      <transfer>
        <destURL>gridftp.bigred.iu.teragrid.org:2812/N/dc/scratch/</destURL>
        <sourceURL>gsiftp://gridftp-abe.ncsa.teragrid.org:2812/cfs/scratch/</sourceURL>
      </transfer>
    </service>
  </services>
</ld:leadDefinition>
