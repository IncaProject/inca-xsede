<sr:statusReport xmlns:sr="http://inca.sdsc.edu/consumer/statusReport_2.0"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <!-- Displays the following version tests in one graph on the page -->
  <graph>
    <!-- Format is:  nickname, resource, label  
         The label can be any unique string within this collection of tests -->
    <chart>metric</chart>
    <metric>Apache_CGI,Apache_static,Apache_xslt_view,Globus_MDS</metric>
    <series>info.teragrid.org_stats,quarry,info.teragrid.org</series>
    <series>info1.dyn.teragrid.org_stats,quarry,info1.dyn.teragrid.org</series>
    <series>info2.dyn.teragrid.org_stats,quarry,info2.dyn.teragrid.org</series>
    <titlePrefix>Information Services (</titlePrefix>
    <titleSuffix>)</titleSuffix>
  </graph>

  <mouseovers>true</mouseovers>
  <links>true</links>
  <width>500</width>
  <height>250</height>

  <pageDescription>The following page displays the performance history of the TeraGrid Information Services servers. To view data from a different start date, use the form below.</pageDescription>
  <pageTitle>TeraGrid - Information Services</pageTitle>
</sr:statusReport>

