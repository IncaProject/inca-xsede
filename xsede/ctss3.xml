                    <exempt>ncsa-ia64</exempt>
                </unitalias>
                <unitalias>
                    <name>tgcp_to_ncsa-ia64_pfs</name>
                    <exempt>ncsa-ia64</exempt>
                </unitalias>
                <unitalias>
                    <name>tgcp_to_ncsa-ia64_scratch</name>
                    <exempt>ncsa-ia64</exempt>
                </unitalias>
                <unitalias>
                    <name>tgcp_to_ornl-login_home</name>
                    <exempt>ornl-login</exempt>
                </unitalias>
                <unitalias>
                    <name>tgcp_to_ornl-login_pfs</name>
