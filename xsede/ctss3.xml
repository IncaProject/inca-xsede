        <package>
            <id>tgusage</id>
            <type>user</type>
            <category>User Environment and Tools</category>
            <description>Command to display usage information on each TeraGrid resource.</description>
            <version>any</version>
            <versionRE>^.*</versionRE>
            <tests>
                <version>
                    <name>tgusage_version</name>
                </version>
                <unitalias>
                    <name>tgusage_grep</name>
                </unitalias>
            </tests>
