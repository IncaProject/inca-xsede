<?xml version = "1.0" encoding = "UTF-8"?>
<stack>
    <id>CTSS 3.0</id>
    <set>
        <id>core</id>
        <package>
            <id>ant</id>
            <type>admin</type>
            <category>OS, installation and drivers</category>
            <description>Apache Ant is a Java-based build tool.</description>
            <docs><!-- <a href="http://ant.apache.org/">Ant Homepage</a> --></docs>
            <description>Apache Ant is a Java-based build tool.</description>
            <docs><!-- <a href="http://ant.apache.org/">Ant Homepage</a> --></docs>
            <version>>=1.6.4</version>
            <versionRE>(^1\.[6-9]\.[4-9].*)|(^[2-9].*)</versionRE>
            <tests>
                <version>ant_version</version>
            </tests>
        </package>
        <package>
            <id>condor-g</id>
            <type>user</type>
            <category>Grid Services</category>
            <class>Grid</class>
            <description>Condor-G combines the inter-domain resource management protocols of the Globus Toolkit and the intra-domain resource and job management methods of Condor to allow the user to harness multi-domain resources as if they all belong to one personal domain. </description>
            <docs><!-- <a href="http://www.cs.wisc.edu/condor/condorg/">Condor-G Homepage</a> --></docs>
            <url>http://repo.teragrid.org/head/open/condor-g</url>
            <version>any</version>
            <versionRE>^.*</versionRE>
            <tests>
                <version>condor_version</version>
                <unitalias>condorq</unitalias>
            </tests>
        </package>
        <package>
            <id>gridshell</id>
            <type>user</type>
            <category>Grid Services</category>
            <description>GridShell extends the Tenex C Shell (TCSH) and Bourne Again Shell (BASH) to transparently incorporate useful distributed and grid computing concepts into the UNIX shell login environment.</description>
            <docs><!-- <a href="http://www.gridshell.net/">GridShell Homepage</a> --></docs>         <version>any</version>
            <versionRE>^.*</versionRE>
            <tests>
                <version>gridshell_version</version>
            </tests>
        </package>
        <package>
            <id>hdf4</id>
            <type>user</type>
            <category>I/O</category>
            <class>Development</class>
            <description>HDF software includes I/O libraries and tools for analyzing, visualizing, and converting scientific data.</description>
            <docs><!-- <a href="http://hdf.ncsa.uiuc.edu/">HDF Homepage</a> --></docs>
            <url>http://repo.teragrid.org/head/open/hdf4</url>
            <version>any</version>
            <versionRE>^.*</versionRE>
            <tests>
                <version>hdf4_version</version>
            </tests>
        </package>
        <package>
            <id>hdf5</id>
            <type>user</type>
            <category>I/O</category>
            <class>Development</class>
            <description>HDF software includes I/O libraries and tools for analyzing, visualizing, and converting scientific data.</description>
            <docs><!-- <a href="http://hdf.ncsa.uiuc.edu/">HDF Homepage</a> --></docs>
            <url>http://repo.teragrid.org/head/open/hdf5</url>
            <version>any</version>
            <versionRE>^.*</versionRE>
            <tests>
                <version>hdf5_version</version>
            </tests>
        </package>
        <package>
            <id>myproxy</id>
            <type>user</type>
            <class>Grid</class>
            <category>Grid Services</category>
            <description>A credential repository for the Grid. </description>
            <docs><!-- <a href="http://grid.ncsa.uiuc.edu/myproxy/">NCSA's MyProxy Information</a> --></docs>
            <version>>=0.6.2</version>
            <versionRE>(^0\.[6-9]\.[2-9].*)|(^[1-9]\.[0-9].*)</versionRE>
            <tests>
                <version>myproxy_version</version>
                <unitalias>myproxy_client</unitalias>
            </tests>
        </package>
        <package>
            <id>ssh</id>
            <type>user</type>
            <category>Grid Services</category>
            <class>Grid</class>
            <docs><!-- <a href="http://www.ssh.com/">OpenSSH Homepage</a> --></docs>
            <url>http://repo.teragrid.org/head/open/ssh</url>
            <version>any</version>
            <versionRE>^.*</versionRE>
            <tests>
                <version>ssh_version</version>
		<unitalias>ssh_to_tg-login.uc.teragrid.org</unitalias>                                         
		<unitalias>ssh_to_tg-viz-login.uc.teragrid.org</unitalias>                                     
		<unitalias>ssh_to_tg-login1.iu.teragrid.org</unitalias>                                         
                <unitalias>ssh_to_ih2.uits.iupui.edu</unitalias>                                                
		<unitalias>ssh_to_cobalt.ncsa.teragrid.org</unitalias>                                          
		<unitalias>ssh_to_copper.ncsa.uiuc.edu</unitalias>                                          
		<unitalias>ssh_to_tg-login.ncsa.teragrid.org</unitalias>                                        
		<unitalias>ssh_to_tund.ncsa.uiuc.edu</unitalias>                                          
		<unitalias>ssh_to_tg-login.ornl.teragrid.org</unitalias>                                        
		<unitalias>ssh_to_tg-login.rachel.psc.teragrid.org</unitalias>                                  
		<unitalias>ssh_to_tg-login1.lemieux.psc.teragrid.org</unitalias>                                
		<unitalias>ssh_to_tg-login.bigben.psc.teragrid.org</unitalias>                                  
		<unitalias>ssh_to_tg-login-linux.purdue.teragrid.org</unitalias>                                
		<unitalias>ssh_to_bglogin.sdsc.edu</unitalias>                                                  
		<unitalias>ssh_to_dslogin.sdsc.edu</unitalias>
		<unitalias>ssh_to_tg-login.sdsc.teragrid.org</unitalias>                                       
		<unitalias>ssh_to_tg-login.tacc.teragrid.org</unitalias>                                       
		<unitalias>ssh_to_tg-viz-login.tacc.teragrid.org</unitalias>                                   
            </tests>
        </package>
        <package>
            <id>pacman</id>
            <type>admin</type>
            <category>OS, installation and drivers</category>
            <description>Pacman defines, installs, configures and sets up software.</description>
            <docs><!-- <a href="http://physics.bu.edu/pacman/">Pacman Homepage</a> --></docs>
            <version>>=3.14.1</version>
            <versionRE>(^3\.[1-9][4-9]\.[1-9].*)|(^3\.[1-9][5-9].*)|(^[4-9].*)</versionRE>
            <tests>
                <version>pacman_version</version>
            </tests>
        </package>
        <package>
            <id>softenv</id>
            <type>user</type>
            <category>User Environment and Tools</category>
            <class>Development</class>
            <description>A system designed to make it easier for users to define what applications they want to use, and easier for administrators to make applications available to users.</description>
            <docs><!-- <a href="http://www.teragrid.org/userinfo/softenv/">SoftEnv Documentation</a> --></docs>
            <url>http://repo.teragrid.org/head/open/softenv</url>
            <version>any</version>
            <versionRE>^.*</versionRE>
            <tests>
                <version>softenv_version</version>
                <unitalias>tg_environ_vars</unitalias>
            </tests>
        </package>
        <package>
            <id>srb-client</id>
            <type>user</type>
            <category>I/O</category>
            <description>The Storage Resource Broker (SRB) is a client-server middleware that provides a uniform interface for connecting to heterogeneous data resources over a network and accessing replicated data sets. </description>
            <docs><!-- <a href="http://www.npaci.edu/DICE/SRB/">SRB Homepage</a> --></docs>
            <version>any</version>
            <versionRE>^.*</versionRE>
            <tests>
                <version>srb_version</version>
                <unitalias>sls</unitalias>
            </tests>
        </package>
        <package>
