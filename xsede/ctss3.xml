        </package>
        <package>
            <id>tgusage</id>
            <type>user</type>
            <category>User Environment and Tools</category>
            <description>Command to display usage information on each TeraGrid resource.</description>
            <version>any</version>
            <versionRE>^.*</versionRE>
            <tests>
                <version>tgusage_version</version>
                <unitalias>tgusage_grep</unitalias>
            </tests>
        </package>
        <package>
            <id>uberftp</id>
            <type>user</type>
            <category>I/O</category>
            <description>Interactive, GridFTP-enabled FTP client.</description>
            <docs><!-- <a href="http://dims.ncsa.uiuc.edu/set/uberftp/">UberFTP Homepage</a> --></docs>
            <url>https://repo.teragrid.org/head/open/uberftp/</url>
            <version>any</version>
            <versionRE>^.*</versionRE>
            <tests>
                <version>uberftp_version</version>
                <unitalias>uberftp</unitalias>
            </tests>
        </package>
    </set>
</stack>
