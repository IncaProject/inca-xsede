            <url>http://repo.teragrid.org/head/open/ssh</url>
            <version>any</version>
            <versionRE>^.*</versionRE>
            <tests>
                <version>
                    <name>ssh_version</name>
                </version>
                <unitalias>
                    <name>ssh_to_tg-login.uc.teragrid.org</name>
                </unitalias>
