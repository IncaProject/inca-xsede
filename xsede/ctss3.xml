            <url>https://repo.teragrid.org/head/open/gridftp/tgcp/</url>
            <version>any</version>
            <versionRE>^.*</versionRE>
            <tests>
                <version>tgcp_version</version>
                <unitalias>tgcp_to_anl-ia64_home</unitalias>
                <unitalias>tgcp_to_anl-ia64_pfs</unitalias>
                <unitalias>tgcp_to_anl-ia64_scratch</unitalias>
